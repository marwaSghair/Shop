{
  "projectType": "library",
  "sourceRoot": "libs/shared/layout/src",
  "prefix": "osf-front",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/layout"],
      "options": {
        "jestConfig": "libs/shared/layout/jest.config.js",
        "passWithNoTests": true
